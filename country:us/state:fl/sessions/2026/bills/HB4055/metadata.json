  "abstracts": [
    {
      "note": "summary",
      "abstract": "City of Lake Wales, Polk County;  Provides exception to general law for licensed alcoholic beverage vendors located within specified area of City of Lake Wales; provides that licensed alcoholic beverage vendors within specified area must comply with all ordinances, statutes, & rules not inconsistent with specified provisions; defines specified area of city to which such exception applies."
    }
  ],
