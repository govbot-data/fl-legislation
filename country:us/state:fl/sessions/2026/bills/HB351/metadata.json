  "abstracts": [
    {
      "note": "summary",
      "abstract": "Concurrent Legislative Jurisdiction over United States Military Installations;  Provides that this state may accept relinquishment of exclusive concurrent legislative jurisdiction from U.S. over U.S. military installations located within boundaries of this state; provides that this state has concurrent legislative jurisdiction with U.S. over such military installations; provides procedures & requirements therefor."
    }
  ],
